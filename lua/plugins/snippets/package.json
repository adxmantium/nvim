{
  "name": "custom nvim snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "markdown"
        ],
        "path": "./markdown.json"
      },
      {
        "language": [
          "html"
        ],
        "path": "./html.json"
      },
      {
        "language": [
          "php"
        ],
        "path": "./php.json"
      }
    ]
  }
}
